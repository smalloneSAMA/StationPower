<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.ssm.mapper.StationTypeMapper" >

    <!--结果映射-->
    <resultMap id="TypeMap" type="com.demo.ssm.po.StationType">
        <id column="ID" property="id" jdbcType="INTEGER"/>
        <result column="Type" property="type" jdbcType="VARCHAR"/>
        <result column="AverageBusinessNum" property="averageBusinessNum" jdbcType="VARCHAR"/>

    </resultMap>

    <!--字段片段-->
    <sql id="Type">
        ID,Type,AverageBusinessNum
    </sql>

    <!--按照ID查询-->
    <select id="selectByPrimaryKey" resultMap="TypeMap" >
        SELECT *
        FROM StationTypeAndBusinessnum

    </select>

    <select id="count" resultType="java.lang.Integer">
        SELECT COUNT(id)
        FROM StationTypeAndBusinessnum

    </select>

</mapper>